    "potions = []\n",
    "f = open('StudentsPerformance.csv')\n",
    "for line in f:\n",
    "    info = line.split(',')\n",
    "    if info[0] == 'gender' or info[0] == 'male':\n",
    "        continue\n",
    "    else:\n",
    "        mark = int(info[6])\n",
    "        potions.append(mark) \n",

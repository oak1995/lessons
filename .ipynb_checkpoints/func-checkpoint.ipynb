    "def get_median(my_list):\n",
    "    median = 0\n",
    "    my_list.sort()\n",
    "    if len(my_list)%2 == 0:\n",
    "        median = (my_list[len(my_list)//2]+my_list[len(my_list)//2 - 1])/2\n",
    "    else:\n",
    "        median = my_list[len(my_list)//2 ]\n",
    "    return median\n",
    "print(get_median([10, 6, 2, 9, 21]))"
   ]

   "source": [
    "n = int(input())\n",
    "a = list(map(int, input().split()))\n",
    "\n",
    "K = sorted(set(a))\n",
    "s = len(K)\n",
    "\n",
    "rank = {val: i+1 for i, val in enumerate(K)}\n",
    "\n",
    "\n",
    "z = [0]*(s) \n",
    "\n",
    "for x in a:\n",
    "    r = rank[x]\n",
    "    B = s - r\n",
    "    z[B] += 1\n",
    "\n",
    "\n",
    "for K in range(n):\n",
    "    if K < s:\n",
    "        print(z[K])\n",
    "    else:\n",
    "        print(0)\n",
    "\n"
   ]
